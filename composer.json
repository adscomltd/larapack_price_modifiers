{
  "name": "adscomltd/larapack_price_modifiers",
  "description": "Adds price modifier feature support",
  "license": "MIT",
  "authors": [
    {
      "name": "Armen Matevosyan",
      "email": "aamatevosyan@edu.hse.ru",
      "homepage": "https://hyperstech.com"
    }
  ],
  "homepage": "https://github.com/adscomltd/larapack_price_modifiers",
  "keywords": [
    "Laravel",
    "LarapackPriceModifiers"
  ],
  "require": {
    "illuminate/support": "~7|~8",
    "torann/currency": "^1.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "~9.0",
    "orchestra/testbench": "~5|~6"
  },
  "autoload": {
    "files": [
      "src/Helpers/helpers.php"
    ],
    "psr-4": {
      "Adscom\\LarapackPriceModifiers\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Adscom\\LarapackPriceModifiers\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Adscom\\LarapackPriceModifiers\\LarapackPriceModifiersServiceProvider"
      ]
    }
  }
}
